<Window x:Class="BannerLordMapDownloader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BannerLordMapDownloader"
        mc:Ignorable="d"
        Title="Bannerlord Map Downloader" HorizontalAlignment="Center" VerticalAlignment="Center"
        SizeToContent="WidthAndHeight" Height="295" ResizeMode="NoResize">
    <Grid Background="#FFA7A7A7" Margin="0,0,0,-6">
        <TextBox Name="Console" HorizontalAlignment="Left" Margin="5,20,5,10" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="775" Height="231" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" Text="Output:&#xD;&#xA;" IsReadOnly="True"/>
        <Button x:Name="Browse" Content="Browse For Json File" HorizontalAlignment="Center" Margin="0,10,0,249" VerticalAlignment="Bottom" Width="775" Height="21" Click="Browse_Click"/>

    </Grid>
</Window>
